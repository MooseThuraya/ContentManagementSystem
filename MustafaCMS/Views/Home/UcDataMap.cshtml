
@{
    ViewBag.Title = "UcDataMap";
    Layout = "~/Views/Shared/_mooseUI.cshtml";
}

<div class="main">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-8 p-r-0 title-margin-right">
                <div class="page-header">
                    <div class="page-title">
                        <h1>Hello, <span>@Session["firstName"].ToString()</span></h1>
                    </div>
                </div>
            </div>
            <!-- /# column -->
            <div class="col-lg-4 p-l-0 title-margin-left">
                <div class="page-header">
                    <div class="page-title">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">Dashboard</a></li>
                            <li class="breadcrumb-item active">UC-Datamap</li>
                        </ol>
                    </div>
                </div>
            </div>
            <!-- /# column -->
        </div>
        <!-- /# row -->

        <section id="main-content">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="datamap">
                            <div id="world-datamap"></div>
                        </div>
                    </div>
                    <!-- /# card -->
                </div>
                <!-- /# column -->
            </div>
            <!-- /# row -->


        </section>
    </div>
    <!--  Datamap -->
    <script src="@Url.Content("~/Content/assets/")js/lib/datamap/d3.min.js"></script>
    <script src="@Url.Content("~/Content/assets/")js/lib/datamap/topojson.js"></script>
    <script src="@Url.Content("~/Content/assets/")js/lib/datamap/datamaps.world.min.js"></script>
    <script src="@Url.Content("~/Content/assets/")js/lib/datamap/datamap-init.js"></script>
    <!-- // Datamap -->
</div>

